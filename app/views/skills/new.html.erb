<h2>Add Skill</h2>

<%= form_for(current_user.skills.build) do |f| %>
	
  <div class="field">
    <%= f.label :skill %><br />
    <%= f.text_field :skill %>
  </div>

   <div class="field">
    <%= f.label :level %><br />
    <%= f.text_field :level %>
  </div>

   <div class="actions">
    <%= f.submit "Add Skill" %>
  </div>

<% end %>